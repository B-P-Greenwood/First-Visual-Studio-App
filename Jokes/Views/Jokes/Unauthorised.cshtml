@model Jokes.Models.Joke

@{
    ViewData["Title"] = "Unauthorised";
}

<h1 id="access-denied-title">Sorry, you are <span id="not-auth">not authorised </span>to do that</h1>
<h4>You have to be the joke author to make any amendments to it.</h4>

<div id="access-denied-frame">
    <img id="access-denied-image" src="~/Images/access_denied.png" />
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
